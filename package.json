{
  "name": "dotcom",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "git://gitlab.com/yumbrands/kfc-commerce/dotcom.git"
  },
  "scripts": {
    "dev": "next",
    "clean": "rimraf .next public/env.js",
    "prepare:env": "node -r dotenv/config scripts/generate-env.js",
    "prepare": "npm run prepare:env && husky install",
    "start": "npm run prepare:env && next start",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build && mv ./.next/analyze ./analyze/report",
    "export": "next export",
    "test": "npm run prepare:env && jest --no-coverage --watch .",
    "test:file": "npm run prepare:env && jest --no-coverage --watch",
    "test:changed": "npm run prepare:env && jest --no-coverage --watch --onlyChanged",
    "test:no-watch": "npm run prepare:env && jest --no-coverage",
    "test:no-watch:related": "npm run prepare:env && jest --no-coverage --findRelatedTests",
    "test:watch:coverage": "npm run prepare:env && jest --watchAll",
    "test:ci": "npm run test:ci:generate && npm run prepare:env && jest --ci .",
    "test:ci:generate": "node scripts/generate-pages-tests.js",
    "test:detect-open": "jest --detectOpenHandles",
    "style:fix-all": "prettier --write **/*.tsx **/*.ts",
    "depcheck": "depcheck",
    "typecheck": "tsc",
    "prettier": "prettier --check **/*.tsx **/*.ts",
    "eslint": "eslint ./__mocks__ ./@types ./pages ./scripts ./src ./state ./utils --fix",
    "pre-commit": "npx lint-staged && tsc --noEmit && npm run depcheck",
    "pre-check": "depcheck && tsc && eslint",
    "mr-pre-check": "npm ci && depcheck && tsc && eslint && npm run prettier && npm run test:ci && echo \"\\033[32mAll good, MR ready!\\033[0m\"",
    "mr-pre-check:local": "depcheck && tsc && eslint && npm run prettier && npm run test:ci && echo \"\\033[32mAll good, MR ready!\\033[0m\"",
    "kill-port-3000": "npx kill-port 3000 && echo \"\\033[32mKill mission successful. Port 3000 good to use!\\033[0m\"",
    "export:auth": "BUILD_AUTH=true next build && next export",
    "link": "npm link @kfc-commerce/kfc-sdk",
    "postbuild": "node scripts/generate-sitemap.js"
  },
  "dependencies": {
    "@amplitude/analytics-browser": "^1.6.6",
    "@apollo/client": "^3.7.3",
    "@contentful/rich-text-html-renderer": "^16.3.0",
